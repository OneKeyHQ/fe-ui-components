{
  "name": "@onekeyhq/ui-components",
  "version": "0.0.27",
  "main": "./index.js",
  "author": "OneKey <hi@onekey.so>",
  "homepage": "https://github.com/onekeyhq/fe-ui-components",
  "description": "OneKey FE UI Components.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/onekeyhq/fe-ui-components.git"
  },
  "bugs": {
    "url": "https://github.com/onekeyhq/fe-ui-components/issues"
  },
  "keywords": [
    "OneKey",
    "react",
    "design",
    "javascript"
  ],
  "scripts": {
    "build:icons": "rm -rf src/components/Icon/react && svgr -d src/components/Icon/react src/svg && node src/components/Icon/script.js",
    "build:components": "tsc && node job/build-npm.js",
    "build:css": "postcss src/components/index.css -o build/components/index.css",
    "build": "rm -rf build && yarn build:components && yarn build:css",
    "eject": "react-scripts eject",
    "storybook": "TAILWIND_MODE=watch start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "type-check": "tsc --noEmit"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@changesets/cli": "^2.17.0",
    "@ethersproject/providers": "^5.4.5",
    "@headlessui/react": "^1.4.0",
    "@web3-react/abstract-connector": "^6.0.7",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.3.1",
    "js-cookie": "^3.0.0",
    "lodash": "^4.17.21",
    "react-image-fallback": "^8.0.0",
    "react-intl": "^5.20.6",
    "react-jazzicon": "^0.1.3",
    "react-select": "^4.3.1",
    "react-svg": "^14.0.7",
    "react-tooltip": "^4.2.21",
    "swr": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/node-logger": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "@storybook/theming": "^6.3.6",
    "@svgr/cli": "^5.5.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.171",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@web3-react/types": "^6.0.7",
    "autoprefixer": "^9",
    "cssnano": "^5.0.7",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.1.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
